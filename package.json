{
  "name": "react-material-dashboard",
  "version": "0.1.0",
  "private": false,
  "description": "OSS Material Design Dashboard",
  "author": {
    "name": "Gustavo Ordaz",
    "email": "ordazsgustavo@gmail.com",
    "url": "https://ordazgustavo.netlify/com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ordazgustavo/react-material-dashboard"
  },
  "license": "MIT",
  "keywords": [
    "React",
    "Material",
    "Dashboard"
  ],
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "apexcharts": "^2.1.8",
    "classnames": "^2.2.6",
    "formik": "^1.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts": "^2.1.3",
    "react-testing-library": "^5.4.4",
    "yup": "^0.26.6"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.+(js|md|css|json)\""
  },
  "devDependencies": {
    "docz": "^0.13.7",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "source-map-explorer": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,md,css,json}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "browserslist": [
    ">0.25%",
    "not op_mini all",
    "ie 11"
  ]
}
